{
    "command": "/usr/bin/octavia-worker --config-file /usr/share/octavia/octavia-dist.conf --config-file /etc/octavia/octavia.conf",
    "config_files": [
        {
            "source": "/var/lib/config-data/merged/octavia.conf",
            "dest": "/etc/octavia/octavia.conf",
            "owner": "octavia",
            "perm": "0600"
        },
        {
            "source": "/var/lib/config-data/merged/custom.conf",
            "dest": "/etc/octavia/octavia.conf.d/custom.conf",
            "owner": "octavia",
            "perm": "0600"
        }
    ]
}
